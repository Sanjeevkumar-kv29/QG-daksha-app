<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_app"
    app:startDestination="@id/auth_activity">

    <activity
        android:id="@+id/auth_activity"
        android:name="com.quickghy.qgdaksha.ui.auth.MainActivity"
        android:label="Auth Activity"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/dash_activity"
        android:name="com.quickghy.qgdaksha.ui.dash.DashActivity"
        android:label="Dash Activity"
        tools:layout="@layout/activity_dash" />

    <action
        android:id="@+id/action_global_dash_activity"
        app:destination="@id/dash_activity"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"
        app:popEnterAnim="@anim/nav_default_pop_enter_anim"
        app:popExitAnim="@anim/nav_default_pop_exit_anim"
        app:launchSingleTop="true" />


</navigation>